<script type="text/javascript" charset="utf-8">
function log(obj){
	try{
		console.log(obj);
	}catch(e){}
}

var Cookie = {
	set: function(name,value,seconds){
		if(seconds){
			d = new Date();
			d.setTime(d.getTime() + (seconds * 1000));
			expiry = '; expires=' + d.toGMTString();
		}else
			expiry = '';
		document.cookie = name + "=" + value + expiry + "; path=/";
	},
	get: function(name){
		nameEQ = name + "=";
		ca = document.cookie.split(';');
		for(i = 0; i < ca.length; i++){
			c = ca[i];
			while(c.charAt(0) == ' ')
				c = c.substring(1,c.length);
			if(c.indexOf(nameEQ) == 0)
				return c.substring(nameEQ.length,c.length);
		}
		return null
	},
	unset: function(name){
		Cookie.set(name,'',-1, 60 * 60 * 24 * 30 * 365);
	}
}

function toggleAbout(){
	clear_selection("menu");
	if($('#about').css('display')=='none'){
		hideOther('about');
		$('#about').show();
		$('#about_btn').addClass('selected');
		$('#embed').focus();
	}else{
		$('#about').hide();
	}
}

function toggleVersionup(){
	clear_selection("menu");
	if($('#versionup').css('display')=='none'){
		$('#versionup').show();
        $('#gyazo').css('opacity', "0.5");
        $('#header').css('opacity', "0.5");
	}else{
		$('#versionup').hide();
        $('#gyazo').css('opacity', "1");
        $('#header').css('opacity',"1");
	}
}
function toggleGyazopro(){
	clear_selection("menu");
	if($('#gyazopro').css('display')=='none'){
		$('#gyazopro').show();
        $('#gyazo').css('opacity', "0.5");
        $('#header').css('opacity', "0.5");
	}else{
		$('#gyazopro').hide();
        $('#gyazo').css('opacity', "1");
        $('#header').css('opacity', "1");
	}
}
function hideOther(name){
	var names = 'twitter flickr about'.split(' ');
	for(var i=0,l=names.length;i<l;i++){
		if(names[i]!=name) $('#'+names[i]).hide();
	}
}

function copyEmbed(){
	$('#embed').select();
//	var swf = "<embed src='/public/swf/setClipboard.swf' FlashVars='code="+encodeURIComponent($('embed').value)+"' width='0' height='0' type='application/x-shockwave-flash'></embed>";
//	$('copy').innerHTML = swf;
}

function clear_selection(id){
	$('#'+id).find("li").each(function(i, e){
		e.className = "";
	});
}


function show_dialog(){
	$('#dialog_frame').show();
	$('#image_header').hide();
	//$('author_tag').focus();
}

function close_dialog(){
	clear_selection("menu");
	hideOther('');
}

function set_tag(tag){
	$('#public_tags')[0].value += tag + ' ';
}



function hide_header(){
	$('#header').hide();
}


function toggleSize(){
	if($('#gyazo_img')[0].style.maxHeight != '600px'){
		$('#gyazo_img')[0].style.maxHeight = '600px';
	}else{
		$('#gyazo_img')[0].style.maxHeight = '400px';
	}
}

function hideNotice(){
	$('#noticebar').hide();
}

function showNotice(){
	$('#noticebar').show();
}


var appearcount = 0;
var ht = 0;
var oldX=0;oldY=0;

function onImgMouseMove(evt){
	if (document.all)
	{
		mouseX = event.x;
		mouseY = event.y;
	}else{
		mouseX = evt.pageX;
		mouseY = evt.pageY;
	}
	if (appearcount > 0){
		if (mouseY < 40){
			showHeader();
			oldX = mouseX;
			oldY = mouseY;
			return;
		}
		return;
	}
	
	showHeader();
	oldX = mouseX;
	oldY = mouseY;
}



function onHeaderMouseOver(){
	clearTimeout(ht);
}
function onHeaderMouseOut(){
	ht = setTimeout(hideHeader,1300);
}

function showHeader(){
	if($('#header')[0].style.display != 'block'){
		$('#header')[0].style.display = 'block';
		clearTimeout(ht);
		ht = setTimeout(hideHeader,1300);
		appearcount++;
	}

}

function hideHeader(){


	$('#header')[0].style.display = 'none';
	
}

function toggleHeader(){
	if($('#header')[0].style.display == 'block'){
		$('#header').hide();
		$('#content')[0].style.marginTop = '0';
	}else{
		$('#header').show();
		$('#content')[0].style.marginTop = '38px';
	}
}

var trycount = 0;
var imgobj;

$(function(){

	$(window).keydown(function(e){
		if(e.keyCode == 27){
			close_dialog();
		}
	});
	$('#gyazo_img').click(function(e){
		toggleHeader();
	});

});


$(function(){
	$('#delete').click(do_delete);
	$('#star').mousedown(toggle_star)
	.mouseover(function(e){
		if(!$(e.target).hasClass('stared')){
			$(e.target).css('background-position', '-22px 1px');
		}
	}).mouseout(function(e){
		if(!$(e.target).hasClass('stared')){
			$(e.target).css('background-position', '2px 2px');
		}
	});
});

function do_delete(e){
	if (isPremium <= 0){
		if(confirm(messages['delete_premium'])){
			location.href = "/premium/";
		};
	
	}else{
		if (confirm(messages['delete_alert']))
		{
			$.ajax({
				type: "POST",
				url: "/edit/do_delete",
				data: "fname="+image_id,
				dataType: "json",
				success: function(data, dataType){
					$('#gyazo_img').fadeOut('fast');
					if (data.stat == "ok"){
						location.href = '/history';
					}else{
						alert(messages['delete_fail'] + data.msg);
					}
				},
				error : function(XMLHttpRequest, textStatus, errorThrown){
					alert(messages['server_error']);
				}
			});
		}
	}
}
function toggle_star(e){
	var star = $(e.target);
	$(star).addClass('prestared').css('background-position', '-21px 2px')
	$.ajax({
		type: "POST",
		url: "/edit/toggle_star",
		data: "fname="+image_id,
		dataType: "json",
		success: function(data, dataType){
			if (data.stat == "ok"){
				if(!star.hasClass("stared")){
					$(star).removeClass('prestared').addClass('stared').css('background-position', '-21px 2px')
				}else{
					$(star).removeClass('prestared').removeClass('stared').css('background-position', '2px 2px')
				}
			}
		},
		error : function(XMLHttpRequest, textStatus, errorThrown){
			alert(messages['server_error']);
		}
	});
}
</script>

</body>
</html>